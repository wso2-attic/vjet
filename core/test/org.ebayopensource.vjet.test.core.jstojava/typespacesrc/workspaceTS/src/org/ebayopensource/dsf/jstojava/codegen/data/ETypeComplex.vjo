vjo.etype("org.ebayopensource.dsf.jstojava.codegen.data.ETypeComplex")
.satisfies("org.ebayopensource.dsf.jstojava.codegen.data.IEType")
.protos( {
    m_value : null, //< private boolean
    m_displayName : null,
    m_msg : "Test", //< public String
    constructs : function (val, displayName) { //< private void contructs(boolean, String)
        this.m_value = val;
        this.m_displayName = displayName;
    },
    //> public boolean
    isWeekday : function () { 
        return this.m_value;
    },
    
    //> public boolean
    isEvenOrdinal : function () {
        return (this.ordinal() % 2 == 0);
    },
    
    //> public String
    getDisplayName : function () {
        return this.name() + " is " + this.m_displayName;
    }
  } 
)
.values({
    MON:[true, "Monday"],
    TUE:[true, "Tuesday"], 
    WED:[true, "Wednesday"],  
    THU:[true, "Thursday"],
    FRI:[true, "Friday"], 
    SAT:[false, "Saturday"],
    SUN:[false, "Sunday"]
})
.endType();
